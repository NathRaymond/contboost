{
    "name": "laravel/laravel",
    "type": "project",
    "description": "The Laravel Framework.",
    "keywords": [
        "framework",
        "laravel"
    ],
    "license": "MIT",
    "require": {
        "php": "^8.0.2",
        "akaunting/laravel-money": "^3.1",
        "akaunting/laravel-setting": "^1.2",
        "arrilot/laravel-widgets": "^3.13",
        "astrotomic/laravel-translatable": "^11.11",
        "awssat/laravel-visits": "^4.0",
        "bacon/bacon-qr-code": "^2.0",
        "barryvdh/laravel-dompdf": "^2.0",
        "butschster/meta-tags": "^2.2",
        "cyrildewit/eloquent-viewable": "^6.1",
        "diglactic/laravel-breadcrumbs": "^8.0",
        "guzzlehttp/guzzle": "^7.2",
        "haruncpi/laravel-option-framework": "^1.0",
        "igaster/laravel-theme": "^2.0",
        "jackiedo/dotenv-editor": "^2.0",
        "jenssegers/agent": "^2.6",
        "laravel/breeze": "^1.14",
        "laravel/framework": "^9.19",
        "laravel/sanctum": "^3.0",
        "laravel/socialite": "^5.6",
        "laravel/tinker": "^2.7",
        "mikemclin/laravel-wp-password": "^2.0",
        "nicolaslopezj/searchable": "^1.13",
        "pragmarx/google2fa-qrcode": "^3.0",
        "shetabit/visitor": "^3.1",
        "spatie/laravel-medialibrary": "^10.7",
        "spatie/laravel-permission": "^5.5",
        "spatie/laravel-sitemap": "^6.2",
        "spatie/laravel-sluggable": "^3.4",
        "srmklive/paypal": "~3.0",
        "stripe/stripe-php": "^10.3",
        "watson/active": "^6.1"
    },
    "require-dev": {
        "barryvdh/laravel-debugbar": "^3.7",
        "barryvdh/laravel-ide-helper": "^2.12",
        "fakerphp/faker": "^1.9.1",
        "laravel/pint": "^1.0",
        "laravel/sail": "^1.0.1",
        "mockery/mockery": "^1.4.4",
        "nunomaduro/collision": "^6.1",
        "phpunit/phpunit": "^9.5.10",
        "spatie/laravel-ignition": "^1.0"
    },
    "autoload": {
        "files": [
            "app/Helpers/Helper.php"
        ],
        "psr-4": {
            "App\\": "app/",
            "Database\\Factories\\": "database/factories/",
            "Database\\Seeders\\": "database/seeders/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi"
        ],
        "post-update-cmd": [
            "@php artisan vendor:publish --tag=laravel-assets --ansi --force"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi"
        ]
    },
    "extra": {
        "laravel": {
            "dont-discover": [
                "barryvdh/laravel-ide-helper"
            ]
        }
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
